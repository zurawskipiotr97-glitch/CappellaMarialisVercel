{
  "hosting": {
    "cleanUrls": true,
    "trailingSlash": false,
    "redirects": [
      { "source": "/pl/:path*", "destination": "/:path*", "permanent": true },
      { "source": "/en/:path*", "destination": "/:path*", "permanent": true }
    ],
    "rewrites": [
      { "source": "/regulamin", "destination": "/pl/regulamin" },
      { "source": "/prywatnosc", "destination": "/pl/prywatnosc" },
      { "source": "/rodo", "destination": "/pl/rodo" },
      { "source": "/dostepnosc", "destination": "/pl/dostepnosc" },
      { "source": "/kontakt", "destination": "/pl/kontakt" },
      { "source": "/terms", "destination": "/en/terms" },
      { "source": "/privacy", "destination": "/en/privacy" },
      { "source": "/GDPR", "destination": "/en/GDPR" },
      { "source": "/accessibility", "destination": "/en/accessibility" },
      { "source": "/contact", "destination": "/en/contact" }
    ]
  }
}
