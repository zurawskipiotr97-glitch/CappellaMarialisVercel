{
  "rewrites": [
    {
      "source": "/:path*",
      "has": [
        {
          "type": "header",
          "key": "user-agent",
          "value": ".*(facebookexternalhit|Facebot|meta-externalagent|whatsapp).*"
        }
      ],
      "destination": "/api/_meta?p=:path*"
    }
  ],
  "redirects": [
    {
      "source": "/index.html",
      "destination": "/",
      "permanent": true
    },
    {
      "source": "/(.*)",
      "has": [
        {
          "type": "host",
          "value": "cappellamarialis.pl"
        }
      ],
      "destination": "https://www.cappellamarialis.pl/$1",
      "permanent": true
    }
  ]
}